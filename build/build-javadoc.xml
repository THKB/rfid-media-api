<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="build">
    <target name="build">
    	
        <javadoc access="package" 
        		 author="true" 
        		 destdir="../doc" 
        		 nodeprecated="false"
        		 nodeprecatedlist="false"
        		 noindex="false" 
    			 nonavbar="false"
        		 notree="false"
        		 source="1.7"
        		 splitindex="true"
        		 use="true"
        		 version="true">
        	
        	<fileset dir="../src" includes="**/*.java"/>
        	
        	<classpath>
        		<fileset dir="../lib/feig_id_isc">
        			<include name="OBIDISC4J.jar"/>
        		</fileset>
        	</classpath>
        	
        </javadoc>
    </target>
</project>